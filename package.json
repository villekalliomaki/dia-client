    "subscriptions-transport-ws": "^0.9.19",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^27.0.7",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
